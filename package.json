{
  "name": "@barelyhuman/wrap",
  "version": "1.0.1",
  "description": "Simple library bundler",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "source/index.js",
  "bin": {
    "wrap": "dist/index.js"
  },
  "repository": "barelyhuman/wrap",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix": "standard --fix",
    "prebuild": "rimraf dist",
    "build": "wrap",
    "prepare": "npm run build; chmod +x dist/index.js; husky install",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "bundler",
    "libraries",
    "javascript",
    "rollup",
    "simple"
  ],
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "license": "MIT",
  "wrap": {
    "buble": false,
    "inferExternal": "true"
  },
  "dependencies": {
    "depdown": "^0.0.3",
    "elegant-spinner": "2",
    "indent-string": "^4",
    "kleur": "^4.1.4",
    "log-update": "^4.0.0",
    "logcons": "^0.0.4",
    "mri": "^1.1.6",
    "once": "^1.4.0",
    "signal-exit": "^3.0.6",
    "sizesnap": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@barelyhuman/wrap": "0.0.6",
    "@rollup/plugin-babel": "^5.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-node-externals": "^3.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3"
  },
  "standard": {
    "ignore": [
      "dist/*"
    ],
    "globals": [
      ""
    ]
  },
  "lint-staged": {
    "*.js": [
      "standard --fix"
    ]
  }
}
